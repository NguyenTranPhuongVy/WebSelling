                </div>
            </nav>
            <!--Content-->
            @RenderBody()
            <!--Content-->
            <footer class="footer">
                <div class="container-fluid">
                    <nav class="pull-left">
                        <ul>
                            <li>
                                <a href="#">
                                    Home
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    Company
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    Portfolio
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    Blog
                                </a>
                            </li>
                        </ul>
                    </nav>
                    <p class="copyright pull-right">
                        &copy;
                        <script>document.write(new Date().getFullYear())</script> <a href="http://www.creative-tim.com">Creative Tim</a>, made with love for a better web
                    </p>
                </div>
            </footer>

        </div>
    </div>
    <!--   Core JS Files   -->
    <script src="~/Content/light-bootstrap-dashboard-master/BS3/assets/js/jquery.3.2.1.min.js" type="text/javascript"></script>
    <script src="~/Content/light-bootstrap-dashboard-master/BS3/assets/js/bootstrap.min.js" type="text/javascript"></script>

    <!--  Charts Plugin -->
    <script src="~/Content/light-bootstrap-dashboard-master/BS3/assets/js/chartist.min.js"></script>

    <!--  Notifications Plugin    -->
    <script src="~/Content/light-bootstrap-dashboard-master/BS3/assets/js/bootstrap-notify.js"></script>

    <!--  Google Maps Plugin    -->
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=YOUR_KEY_HERE"></script>

    <!-- Light Bootstrap Table Core javascript and methods for Demo purpose -->
    <script src="~/Content/light-bootstrap-dashboard-master/BS3/assets/js/light-bootstrap-dashboard.js?v=1.4.0"></script>

    <!-- Light Bootstrap Table DEMO methods, don't include it in your project! -->
    <script src="~/Content/light-bootstrap-dashboard-master/BS3/assets/js/demo.js"></script>

    <script type="text/javascript">
    $(document).ready(function () {

            demo.initChartist();

            $.notify({
                icon: 'pe-7s-gift',
                message: "Welcome to <b>Light Bootstrap Dashboard</b> - a beautiful freebie for every web developer."

            }, {
                type: 'info',
                timer: 4000
            });

        });</script>
</body>
</html>