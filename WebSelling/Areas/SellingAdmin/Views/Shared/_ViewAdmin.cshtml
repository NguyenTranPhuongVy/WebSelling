    <!--     Fonts and icons     -->
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>
    <link href="~/Content/light-bootstrap-dashboard-master/BS3/assets/css/pe-icon-7-stroke.css" rel="stylesheet" />

</head>
<body>
    <div class="wrapper">
        <div class="sidebar" data-color="green" data-image="~/Content/light-bootstrap-dashboard-master/BS3/assets/img/sidebar-1.jpg">
            <div class="sidebar-wrapper">
                <div class="logo">
