</a></h1>
            </div>
            <!-- //logo -->
            <!-- nav -->
            <div class="nav_w3ls">
                <nav>
                    <label for="drop" class="toggle">Menu</label>
                    <input type="checkbox" id="drop" />
                    <ul class="menu">
                        <li><a href="@Url.Action("Index", "Home")" class="active"><i class="fa fa-home"></i> Trang Chủ</a></li>
                        @if (user == null)
                        {
                            <li><a href="@Url.Action("Login", "Account")"><i class="fa fa-product-hunt"></i> Sản Phẩm Của Tôi</a></li>
                            <li><a href="@Url.Action("Login", "Account")"><i class="fa fa-comments"></i> Tin Nhắn</a></li>
                            <li><a href="@Url.Action("Login", "Account")"><i class="fa fa-bell"></i> Thông Báo</a></li>
                        }
                        else
                        {
                            <li><a href="@Url.Action("ListProductsUser", "Account")"><i class="fa fa-product-hunt"></i> Sản Phẩm Của Tôi</a></li>
                            <li><a href="pricing.html"><i class="fa fa-comments"></i> Tin Nhắn</a></li>
                            <li><a href="contact.html"><i class="fa fa-bell"></i> Thông Báo</a></li>
                        }
                        <li>
                            <!-- First Tier Drop Down -->
                            <label for="drop-2" class="toggle toogle-2">
                                Tài Khoản <span class="fa fa-angle-down" aria-hidden="true"></span>
                            </label>
                            @if (user == null)
                            {
                                <a href="#"><i class="fa fa-user-circle"></i> Tài Khoản <span class="fa fa-angle-down" aria-hidden="true"></span></a>
                                <input type="checkbox" id="drop-2" />
                                <ul>
                                    <li><a href="@Url.Action("Login", "Account")" class="drop-text"><i class="fa fa-sign-in"></i> Đăng Ký / Đăng Nhập</a></li>
                                    <li><a href="#" class="drop-text"><i class="fa fa-heartbeat"></i> Yêu Thích</a></li>
                                    <li><a href="404.html" class="drop-text"><i class="fa fa-gift"></i> Ưu Đãi</a></li>
                                    <li><a href="#stats" class="drop-text"><i class="fa fa-star"></i> Voucher</a></li>
                                    <li><a href="about.html" class="drop-text"><i class="fa fa-life-saver"></i> Trợ Giúp</a></li>
                                </ul>
                            }
                            else
                            {
                                <a href="@Url.Action("MyInforUser", "Information")"><i class="fa fa-user-circle"></i> @user.User_Name <span class="fa fa-angle-down" aria-hidden="true"></span></a>
                                <input type="checkbox" id="drop-2" />
                                <ul>
                                    <li><a href="@Url.Action("Info", "Account")" class="drop-text"><i class="fa fa-pencil"></i>Thông Tin Cá Nhân</a></li>
                                    <li><a href="@Url.Action("ListProductsUser", "Account")" class="drop-text"><i class="fa fa-heartbeat"></i> Yêu Thích</a></li>
                                    <li><a href="404.html" class="drop-text"><i class="fa fa-gift"></i> Ưu Đãi</a></li>
                                    <li><a href="#stats" class="drop-text"><i class="fa fa-star"></i> Voucher</a></li>
                                    <li><a href="about.html" class="drop-text"><i class="fa fa-life-saver"></i> Trợ Giúp</a></li>
                                    <li><a href="@Url.Action("Logout", "Account")" class="drop-text"><i class="fa fa-sign-out"></i> Đăng Xuất</a></li>
                                </ul>
                            }
                        </li>
                    </ul>
                </nav>
            </div>
            <!-- //nav -->
            <div class="d-flex mt-lg-1 mt-sm-2 mt-3 justify-content-center">
                <!-- search -->
                <div class="ui search search-w3layouts mr-3">
                    <form action="#" method="post" class="search-bottom-wthree d-flex">
                        <input class="search prompt" type="search" placeholder="Tìm Kiếm..." required="" style="border-radius: 50px"> &ensp;
                        <button class="form-control btn" type="submit" style="border-radius: 100%; background-color: #fff"><span class="fa fa-search" style="color: #000000"></span></button>
                        @if (user != null)
                        {
                            <a href="@Url.Action("IndexCart", "Invoices")">
                                <button type="button" class="form-control btn" style="border-radius: 80%; background-color: #fff"><span class="fa fa-cart-arrow-down" style="color: #000000"></span></button>
                            </a>
                        }
                        else
                        {
                            <button class="form-control btn" type="button" style="border-radius: 100%; background-color: #fff"><span class="fa fa-cart-arrow-down" style="color: #000000"></span></button>
                        }
                    </form>
                    <script>
                        @Html.Action("SematicSearch", "Search")
                    </script>
                    <script>
                        $('.ui.search')
                            .search({
                                source: content,
                                maxResults: 20
                            })
                            ;
                    </script>
                </div>
                <!-- //search -->
            </div>
        </div>
    </div>
</header>

