@{
    var a = "@";
}
<script>
$('.ui.form')
    .form({
        fields: {
            User_Email: {
                identifier: 'User_Email',
                rules: [
                    {
                        type: 'empty',
                        prompt: 'Email trống! Vui lòng nhập email'
                    },
                    {
                        type: 'email',
                        prompt: 'Email không đúng định dạng'
                    }
                ]
            },
            User_Pass: {
                identifier: 'User_Pass',
                rules: [
                    {
                        type: 'empty',
                        prompt: 'Mật khẩu trống! Vui lòng nhập mật khẩu'
                    },
                    {
                        type: 'regExp[^(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[!@a#\$%\^&\*])(?=.{8,})]',
                        prompt: 'Mật khẩu phải có ít nhất 8 ký tự bao gồm(Viết hoa, viết thường, ký tự số và ký tự đặt biệt),'
                    }
                ]
            },
            User_Repeats: {
                identifier: 'User_Repeats',
                rules: [
                    {
                        type: 'empty',
                        prompt: 'Vui lòng xác nhận lại mật khẩu.'
                    },
                    {
                        type: 'match[User_Pass]',
                        prompt: 'Mã xác nhận không chính xác! Vui lòng kiểm tra lại.'
                    }
                ]
            },
            //Thay đổi mật khẩu
            OldPass: {
                    identifier: 'OldPass',
                    rules: [
                        {
                            type: 'empty',
                            prompt: 'Không Được Để Trống'
                        }
                    ]
                },
                NewPass: {
                    identifier: 'NewPass',
                    rules: [
                        {
                            type: 'empty',
                            prompt: 'Không Được Để Trống'
                        },
                        {
                            type: 'regExp[^(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[!@a#\$%\^&\*])(?=.{8,})]',
                            prompt: 'Mật Khẩu Ít Nhất Phải Có 8 Ký Tự Bao Gồm(Viết Hoa, Viết Thường, Ký Tự Số Và Ký Tự Đặc Biệt)'
                        }
                    ]
                },
                RePass: {
                    identifier: 'RePass',
                    rules: [
                        {
                            type: 'match[NewPass]',
                            prompt: 'Mật khẩu Nhập Lại Không Khớp'
                        },
                        {
                            type: 'empty',
                            prompt: 'Vui Lòng Xác Nhận Lại Mật Khẩu'
                        }
                    ]
                },
            //Thêm sản phẩm
            Product_Name: {
                identifier: 'Product_Name',
                rules: [
                    {
                        type: 'empty',
                        prompt: 'Bạn Chưa Nhập Tên Sản Phẩm'
                    }
                ]
            },
            fileupload: {
                identifier: 'fileupload',
                rules: [
                    {
                        type: 'empty',
                        prompt: 'Bắt Buộc Phải Có Hình'
                    }
                ]
            },
            Product_Price: {
                identifier: 'Product_Price',
                rules: [
                    {
                        type: 'empty',
                        prompt: 'Giá Không Được Để Trống'
                    },
                    {
                        type: 'integer[0..1000000000000000000000000000000000000000000]',
                        prompt: 'Vui Lòng Nhập Giá Tiền Hợp Lệ'
                    }
                ]
            },
            Category_ID: {
                identifier: 'Category_ID',
                rules: [
                    {
                        type: 'empty',
                        prompt: 'Không Đươc Phép Để Trống'
                    }
                ]
            },
            Product_Description: {
                identifier: 'Product_Description',
                rules: [
                    {
                        type: 'empty',
                        prompt: 'Không Đươc Phép Để Trống'
                    }
                ]
            },
            Product_Detail: {
                identifier: 'Product_Detail',
                rules: [
                    {
                        type: 'empty',
                        prompt: 'Không Đươc Phép Để Trống'
                    }
                ]
            },
            //Thanh Toán
            User_LastName: {
                identifier: 'User_LastName',
                rules: [
                    {
                        type: 'empty',
                        prompt: 'Không Đươc Phép Để Trống'
                    }
                ]
            },
            User_Name: {
                identifier: 'User_Name',
                rules: [
                    {
                        type: 'empty',
                        prompt: 'Không Đươc Phép Để Trống'
                    }
                ]
            },
            User_Phone: {
                identifier: 'User_Phone',
                rules: [
                    {
                        type: 'empty',
                        prompt: 'Không Đươc Phép Để Trống'
                    }
                ]
            },
            City_ID: {
                identifier: 'City_ID',
                rules: [
                    {
                        type: 'empty',
                        prompt: 'Không Đươc Phép Để Trống'
                    }
                ]
            },
            Province_ID: {
                identifier: 'Province_ID',
                rules: [
                    {
                        type: 'empty',
                        prompt: 'Không Đươc Phép Để Trống'
                    }
                ]
            },
            User_Address: {
                identifier: 'User_Address',
                rules: [
                    {
                        type: 'empty',
                        prompt: 'Không Đươc Phép Để Trống'
                    }
                ]
            }






        }
    })
    ;
</script>

